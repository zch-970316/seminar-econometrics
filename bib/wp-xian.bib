@article{bakolis2021,
  title = {Changes in Daily Mental Health Service Use and Mortality at the Commencement and Lifting of {{COVID-19}} `Lockdown' Policy in 10 {{UK}} Sites: A Regression Discontinuity in Time Design},
  shorttitle = {Changes in Daily Mental Health Service Use and Mortality at the Commencement and Lifting of {{COVID-19}} `Lockdown' Policy in 10 {{UK}} Sites},
  author = {Bakolis, Ioannis and Stewart, Robert and Baldwin, David and Beenstock, Jane and Bibby, Paul and Broadbent, Matthew and Cardinal, Rudolf and Chen, Shanquan and Chinnasamy, Karthik and Cipriani, Andrea and Douglas, Simon and Horner, Philip and Jackson, Caroline A. and John, Ann and Joyce, Dan W. and Lee, Sze Chim and Lewis, Jonathan and McIntosh, Andrew and Nixon, Neil and Osborn, David and Phiri, Peter and Rathod, Shanaya and Smith, Tanya and Sokal, Rachel and Waller, Rob and Landau, Sabine},
  year = {2021},
